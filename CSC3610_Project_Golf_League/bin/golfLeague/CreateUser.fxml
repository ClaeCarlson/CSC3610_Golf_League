<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="golfLeague.CreateUserController">
   <center>
      <AnchorPane prefHeight="350.0" prefWidth="426.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="txtUser" layoutX="156.0" layoutY="113.0" onAction="#intialize" />
            <TextField fx:id="txtPass" layoutX="156.0" layoutY="149.0" onAction="#intialize" />
            <TextField fx:id="txtPass2" layoutX="156.0" layoutY="186.0" onAction="#intialize" />
            <Label layoutX="43.0" layoutY="118.0" text="Username" />
            <Label layoutX="44.0" layoutY="154.0" text="Password" />
            <Label layoutX="43.0" layoutY="191.0" text="Confirm Password" />
            <Label layoutX="47.0" layoutY="231.0" text="User Type" />
            <ComboBox fx:id="cmboType" layoutX="156.0" layoutY="226.0" onAction="#intialize" prefHeight="27.0" prefWidth="166.0" />
            <Label layoutX="74.0" layoutY="49.0" prefHeight="34.0" prefWidth="274.0" text="Create New User">
               <font>
                  <Font size="28.0" />
               </font>
            </Label>
            <Button fx:id="btnSubmit" layoutX="261.0" layoutY="266.0" mnemonicParsing="false" onAction="#intialize" text="Submit" />
            <Button fx:id="btnClear" layoutX="156.0" layoutY="266.0" mnemonicParsing="false" onAction="#intialize" text="Clear" />
            <Label fx:id="lblFields" layoutX="157.0" layoutY="302.0" prefHeight="17.0" prefWidth="165.0" text="Required fields left blank!" textFill="RED" />
            <Label fx:id="lblPass" layoutX="156.0" layoutY="325.0" prefHeight="17.0" prefWidth="158.0" text="Passwords don't match!" textFill="RED" />
         </children>
      </AnchorPane>
   </center>
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>