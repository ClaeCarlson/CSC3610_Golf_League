      <Label layoutX="238.0" layoutY="105.0" prefHeight="15.0" prefWidth="18.0" text="\@">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="time" layoutX="268.0" layoutY="103.0" onAction="#initialize" prefHeight="27.0" prefWidth="86.0" promptText="Time" />
      <Button fx:id="logout" layoutX="528.0" layoutY="9.0" mnemonicParsing="false" onAction="#initialize" prefHeight="20.0" prefWidth="61.0" text="Logout" />
   </children>
</AnchorPane>
