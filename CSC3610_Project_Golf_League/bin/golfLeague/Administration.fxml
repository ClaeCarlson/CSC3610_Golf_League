<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="golfLeague.Administration_Controller">
   <children>
      <ImageView fitHeight="400.0" fitWidth="601.0" pickOnBounds="true">
         <image>
            <Image url="@../../bin/golfLeague/Light-Grey-Background-Hd-5-1.jpg" />
