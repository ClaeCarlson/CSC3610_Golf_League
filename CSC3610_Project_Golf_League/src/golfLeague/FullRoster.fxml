<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="golfLeague.FullRoster">
   <center>
      <AnchorPane prefHeight="400.0" prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
               <image>
                  <Image url="@../../bin/golfLeague/Light-Grey-Background-Hd-5-1.jpg" />
               </image>
            </ImageView>
            <TreeTableView fx:id="treeView" layoutX="16.0" layoutY="23.0" prefHeight="342.0" prefWidth="550.0">
              <columns>
                <TreeTableColumn fx:id="name" prefWidth="75.0" text="Name" />
                <TreeTableColumn fx:id="handicap" prefWidth="75.0" text="Handicap" />
                  <TreeTableColumn fx:id="team" prefWidth="75.0" text="Team" />
                  <TreeTableColumn fx:id="score" prefWidth="75.0" text="Score" />
                  <TreeTableColumn fx:id="rank" prefWidth="75.0" text="Rank" />
              </columns>
            </TreeTableView>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
